@-webkit-keyframes pulse { 
  10% { opacity: 0.1; transform: scale(1.5); }
  50% { opacity: 0.2; transform: scale(1.8); }
  70% { opacity: 0.1; transform: scale(1.5); }
  100% { opacity: 0; transform: scale(1); }
}

@keyframes pulse { 
  10% { opacity: 0.1; transform: scale(1.5); }
  50% { opacity: 0.2; transform: scale(1.8); }
  70% { opacity: 0.1; transform: scale(1.5); }
  100% { opacity: 0; transform: scale(1); }
}

@-webkit-keyframes fadein { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

@keyframes fadein { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

@-webkit-keyframes pulse-blur { 
  0% { opacity: 0; transform: translate(-50%, 0px) scale(0); }
  40% { opacity: 1; }
  100% { opacity: 0; transform: translate(-50%, 0px) scale(1.1); }
}

